//
// WORR game definition file (.fgd)
//
// Originally written by autolycus for Quake II, extended for the rerelease by
// id Software and now expanded to cover the full WORR entity catalogue.
// Special thanks to the original maintainers: Disruptor, Zoid, Zaphod, Imaginos,
// EutecTic, xaGe, and Paril.
//
// Includes stuff from the tools for lighting etc

//
// worldspawn
//

@SolidClass base(EWT_base_Worldspawn) = worldspawn : "World entity"
[
    sky(string) : "Environment map name"
    skyaxis(vector) : "Vector axis for rotating sky"
    skyrotate(string) : "Speed of rotation (degrees/second)"
    skyautorotate(integer) : "Disable to set sky rotation manually" : 1
    sounds(integer) : "CD Track Number" : 1
    music(string) : "Music track to play, example : music/track06.ogg"
    gravity(integer) : "Gravity" : 800
    instantitems(integer) : "Instant Item Use" : 0
    hub_map(integer) : "Marks this map as a campaign hub" : 0
    message(string) : "Level name"
    author(string) : "Primary author credit"
    author2(string) : "Secondary author credit"
    nextmap(string) : "Next map (DM only)"
    start_items(string) : "Starting items: a semi-colon delimited list of items and amounts to be given to player if they have a clear inventory."
    achievement(string) : "If an EOU is triggered from this map, this is the achievement to give out."
    ruleset(string) : "Preferred gameplay ruleset alias (q1/q2/q3a)"
    arena(integer) : "Number of discrete arenas for round-based modes" : 0
    no_grapple(integer) : "Disable grappling hook (set to 1)" : 0
    no_dm_spawnpads(integer) : "Disable WORR deathmatch spawn pads (set to 1)" : 0
    no_dm_telepads(integer) : "Disable WORR teleporter pads (set to 1)" : 0
    noBots(integer) : "Prevent bots from spawning (set to 1)" : 0
    noHumans(integer) : "Prevent human players from spawning (set to 1)" : 0
    health_multiplier(float) : "Global monster health multiplier" : 1.0
    fog_density(float) : "density value of fog, 0-1"
    fog_color(color) : "color value of fog, 3d vector with values between 0-1 (r g b)"
    fog_sky_factor(float) : "sky factor value of fog, 0-1"
    heightfog_falloff(float) : "falloff value of heightfog, 0-1"
    heightfog_density(float) : "density value of heightfog, 0-1"
    heightfog_start_color(color) : "start color of heightfog, 3d vector with values between 0-1 (r g b)"
    heightfog_end_color(color) : "end color of heightfog, 3d vector with values between 0-1 (r g b)"
    heightfog_start_dist(float) : "start distance value of heightfog, in world units"
    heightfog_end_dist(float) : "start distance value of heightfog, in world units"
    goals(string) : "Default target list for campaign sequencing"
    image(string) : "Image material for world text overlays"
    fade_start_dist(integer) : "Default info_world_text fade start distance" : 96
    fade_end_dist(integer) : "Default info_world_text fade end distance" : 384
    reinforcements(string) : "Comma separated reinforcement script identifiers"
    noise_start(string) : "Loop start sound"
    noise_middle(string) : "Loop middle sound"
    noise_end(string) : "Loop end sound"
    loop_count(integer) : "Loop count for scripted ambient sequences" : 0
    cvar(string) : "Server cvar to toggle on level start"
    cvarValue(string) : "Value to force when cvar key is provided"
]

