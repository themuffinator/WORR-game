		0  : "Compiler settings"
		1  : "Force on"
	]
	_sunlight_dirt(choices) : "Sunlight dirt" : 0 : "1 enables dirtmapping (ambient occlusion) on sunlight, -1 to disable (making it illuminate the dirtmapping shadows). Default is to use the value of '_dirt'" =
	[
		-1 : "Force off"
		0  : "Compiler settings"
		1  : "Force on"
	]
